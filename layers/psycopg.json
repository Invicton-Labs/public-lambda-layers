{
    "default_layer_source_directory": "/layer",
    "common_instructions_pre": [
        "RUN pip3 install --upgrade pip",
        "WORKDIR /layer"
    ],
    "common_instructions_post": [
        "RUN pip freeze --path . > requirements.txt"
    ],
    "runtimes": {
        "python3.12": {
            "default_image": "public.ecr.aws/lambda/python:3.12",
            "default_layer_target_directory": "/python/lib/python3.12/site-packages",
            "versions": {
                "2.9.10": {
                    "$ref": "#/$defs/version-2.9.10"
                }
            }
        },
        "python3.13": {
            "default_image": "public.ecr.aws/lambda/python:3.13",
            "default_layer_target_directory": "/python/lib/python3.13/site-packages",
            "versions": {
                "3.3.2": {
                    "$ref": "#/$defs/version-3.3.2"
                }
            }
        },
        "python3.14": {
            "default_image": "public.ecr.aws/lambda/python:3.14",
            "default_layer_target_directory": "/python/lib/python3.14/site-packages",
            "versions": {
                "3.3.2": {
                    "$ref": "#/$defs/version-3.3.2"
                }
            }
        }
    },
    "$defs": {
        "version-2.9.3": {
            "common_instructions_pre": [
                "RUN pip install psycopg2-binary==2.9.3 --target ."
            ],
            "architectures": {
                "x86_64": {},
                "arm64": {}
            }
        },
        "version-2.9.10": {
            "common_instructions_pre": [
                "RUN pip install psycopg2-binary==2.9.10 --target ."
            ],
            "architectures": {
                "x86_64": {},
                "arm64": {}
            }
        },
        "version-3.0.14": {
            "common_instructions_pre": [
                "RUN pip install psycopg[binary]==3.0.14 psycopg-pool==3.1.1 --target ."
            ],
            "architectures": {
                "x86_64": {}
            }
        },
        "version-3.3.2": {
            "common_instructions_pre": [
                "RUN pip install psycopg[binary,pool]==3.3.2 --target ."
            ],
            "architectures": {
                "x86_64": {}
            }
        }
    }
}